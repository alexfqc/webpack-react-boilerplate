{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate project that contains webpack setup, react, es6, eslint, sass-lint and production bundle setup",
  "main": "index.jsx",
  "scripts": {
    "start": "./node_modules/babel-cli/bin/babel-node.js server.js",
    "test": "eslint ./public/js/**/*  && sass-lint './public/scss/**/*.scss' -v -q",
    "dev": "NODE_ENV=development webpack",
    "build": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexfqc/webpack-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "es6",
    "sass",
    "sass-lint",
    "eslint"
  ],
  "author": "Alessander Costa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexfqc/webpack-react-boilerplate/issues"
  },
  "homepage": "https://github.com/alexfqc/webpack-react-boilerplate#readme",
  "dependencies": {
    "express": "^4.15.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "path": "^0.12.7",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.3",
    "sasslint-webpack-plugin": "^1.0.4",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.2"
  }
}
